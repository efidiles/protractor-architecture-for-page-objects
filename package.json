{
  "name": "protractor-element-click-and-navigate",
  "version": "1.0.0",
  "description": "Add a clickAndNavigate method to protractor element to use with buttons performing route changes",
  "main": "index.js",
  "scripts": {
    "start": "gulp webserver",
    "test": "npm --silent run test:e2e",
    "test:e2e": "gulp e2e",
    "test:e2e:webdriver": "webdriver-manager start",
    "test:e2e:protractor": "protractor protractor.config.js",
