  },
  "homepage": "https://github.com/efidiles/protractor-element-click-and-navigate#readme",
  "devDependencies": {
    "express": "^4.13.4",
    "gulp": "^3.9.1",
    "gulp-util": "^3.0.7",
    "protractor": "^3.2.2",
    "run-sequence": "^1.1.5"
  }
}
